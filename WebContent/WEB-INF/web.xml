<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>moimproject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MoimFrontController</servlet-name>
    <servlet-class>com.project.app.moim.MoimFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MoimFrontController</servlet-name>
    <url-pattern>*.mo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserFrontController</servlet-name>
    <servlet-class>com.project.app.user.UserFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserFrontController</servlet-name>
    <url-pattern>*.us</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardFrontController</servlet-name>
    <servlet-class>com.project.app.board.BoardFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardFrontController</servlet-name>
    <url-pattern>*.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IndexFrontController</servlet-name>
    <servlet-class>com.project.app.index.dao.IndexFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexFrontController</servlet-name>
    <url-pattern>*.ix</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminFrontController</servlet-name>
    <servlet-class>com.project.app.admin.AdminFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminFrontController</servlet-name>
    <url-pattern>*.ad</url-pattern>
  </servlet-mapping>
</web-app>